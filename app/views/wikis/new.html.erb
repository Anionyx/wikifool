<h1>New Wiki</h1>

<div class="col-md-8">
  <%= form_for @wiki do |f| %>
    <div class="form-group">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div class="form-group">
      <%= f.label :description %>
      <%= f.text_area :description, rows: 10 %>
    </div>
    <%= f.submit "Save", class: 'btn btn-success' %>
  <% end %>
</div>

