<h1><%=  @wiki.title%></h1>
<div class="row">
  <div class="col-md-8">
  <p><%= image_tag(@wiki.image_url) if @wiki.image? %></p>
    <small>
      <%= image_tag(@wiki.user.avatar.tiny.url) if @wiki.user.avatar? %> submitted <%= time_ago_in_words(@wiki.created_at) %> ago by <%= @wiki.user.name %><br/>
    </small>
  </div>
    <%= @wiki.description%>
</div>
<%= link_to "Edit Wiki", edit_wiki_path, class: 'btn btn-success' %>
