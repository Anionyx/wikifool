<h1>Wikis</h1>

<div class="row">
  <div class="col-md-8">
    <% @wikis.each do |wikis| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wikis.title, wikis %>
          </h4>
          <small>
            <%= wikis.description %>
          </small>
        </div>
      </div>
    <% end %>
    <%= will_paginate @wikis %>
  </div>
</div>